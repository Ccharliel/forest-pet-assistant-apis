{
  "rewrites": [
    {
      "source": "/",
      "destination": "/HLSplayer.html"
    },
    {
      "source": "/(.*)",
      "destination": "/HLSplayer.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    }
  ]
}